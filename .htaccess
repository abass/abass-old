# Compression
<FilesMatch "\.(css|js|xhtml|html|htm|php)$">
    SetOutputFilter DEFLATE
</FilesMatch>

# Error 404 Pages
ErrorDocument 400 /
ErrorDocument 401 /
ErrorDocument 403 /
ErrorDocument 404 /
ErrorDocument 500 /

# Libwww-pearl Access Test
RewriteCond %{HTTP_USER_AGENT} libwww-perl.*
RewriteRule .* â€“ [F,L]

# index.php [301 redirect]
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{THE_REQUEST} ^[A-Z]{3,9}\ /index\.html\ HTTP/
RewriteRule ^index\.html$ http://abass.co/ [R=301,L]

#RewriteBase /
#RewriteRule ^(.*)\.html$ $1.php [R=301,L]

# REMOVE FILE EXTENSIONS
RewriteEngine On
# browser requests PHP
RewriteCond %{THE_REQUEST} ^[A-Z]{3,9}\ /([^\ ]+)\.php
RewriteRule ^/?(.*)\.php$ /$1 [L,R=301]
# check to see if the request is for a PHP file
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^/?(.*)$ /$1.php [L]

# www.abass.co to abass.co [301 redirect]
RewriteEngine On
RewriteCond %{HTTP_HOST} http://www.abass.co/
RewriteRule (.*) ^abass.co$1 [R=301,L]

# Denies Access to File Structure
Options -Indexes